{
  "name": "voicediary",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cy:open": "cypress open --e2e",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:run": "firebase emulators:exec --import=./saved-data 'npm start'"
  },
  "dependencies": {
    "@speechly/react-client": "^2.2.0",
    "@speechly/speech-recognition-polyfill": "^1.2.0",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-calendar": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "react-speech-recognition": "^3.10.0",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.24",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "^18.0.8",
    "@types/react-speech-recognition": "^3.9.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^11.2.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  }
}
